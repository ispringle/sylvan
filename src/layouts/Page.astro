---
import Article from "@components/Article.astro";
import Base from "@layouts/Base.astro";

const { frontmatter, ...props } = Astro.props;
---
<Base pageTitle={frontmatter?.title || ""}>
  <Article author={frontmatter?.author || ""} created={frontmatter?.created || ""} updated={frontmatter?.updated || ""}>
    <slot />
  </Article>
</Base>
